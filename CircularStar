import turtle 
import math
bob=turtle.Turtle()
bob.shape("turtle")
bob.color("#1B7F7B")
bob.speed(20)  

# bob.begin_fill()
for i in range(160):
    bob.forward(math.sqrt(i)*50)
    bob.left(145)

     
#bob.end_fill()





turtle.done()
turtle.bye()

